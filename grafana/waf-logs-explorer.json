          "datasource": {
            "type": "grafana-athena-datasource",
            "uid": "DS_WAF_DATA_LAKE"
          },
          "format": 1,
          "rawSQL": "SELECT\n  from_unixtime(\"timestamp\"/1000) AS event_time,\n  action,\n  httprequest.clientip AS client_ip,\n  httprequest.country AS country,\n  httprequest.httpmethod AS method,\n  httprequest.host AS origin,\n  httprequest.uri AS uri,\n  terminatingruleid,\n  terminatingruletype,\n  httprequest.requestid AS request_id\nFROM waf_data_lake.logs\nWHERE \n  $__timeFilter(from_unixtime(\"timestamp\"/1000))\n  \n  -- Filtros din√¢micos\n  AND ('${ip}' = '' OR httprequest.clientip = '${ip}')\n  AND ('${uri}' = '' OR httprequest.uri LIKE '%${uri}%')\n  AND ('${rule}' = '' OR terminatingruleid = '${rule}')\n  AND ('${action}' = '' OR action = '${action}')\n  AND ('${country}' = '' OR httprequest.country = '${country}')\n  AND ('${method}' = '' OR httprequest.httpmethod = '${method}')\n  AND ('${origin}' = '' OR httprequest.host = '${origin}')\n\nORDER BY event_time DESC\nLIMIT ${row_limit}",
          "refId": "A",
          "table": "logs"
        }
      ],
      "title": "WAF Logs Explorer",
      "type": "table"
    }
